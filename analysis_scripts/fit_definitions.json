{
  "BSA": {
    "expression": "ALU_offset + ALU_sinphi*sin(phi)",
    "parameters": ["ALU_offset", "ALU_sinphi"],
    "initial_values": [0, 0],
    "depolarization_factors": ["DepW"]
  },
  "TSA": {
    "expression": "AUL_offset + AUL_sinphi*sin(phi) + AUL_sin2phi*sin(2*phi)",
    "parameters": ["AUL_offset", "AUL_sinphi", "AUL_sin2phi"],
    "initial_values": [0, 0, 0],
    "depolarization_factors": ["DepV", "DepB"]
  },
  "DSA": {
    "expression": "ALL + ALL_cosphi*cos(phi)",
    "parameters": ["ALL", "ALL_cosphi"],
    "initial_values": [0, 0],
    "depolarization_factors": ["DepC", "DepW"]
  },
  "Spin-independent": {
    "expression": "AUU_cosphi*cos(phi) + AUU_cos2phi*cos(2*phi)",
    "parameters": ["AUU_cosphi", "AUU_cos2phi"],
    "initial_values": [0, 0],
    "depolarization_factors": ["DepV", "DepB"]
  }
}
